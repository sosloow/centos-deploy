[nodesource]
name=Node.js Packages for Enterprise Linux 5 - $basearch
baseurl=https://rpm.nodesource.com/pub/el/5/$basearch
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/NODESOURCE-GPG-SIGNING-KEY-EL

[nodesource-source]
name=Node.js for Enterprise Linux 5 - $basearch - Source
baseurl=https://rpm.nodesource.com/pub/el/5/SRPMS
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/NODESOURCE-GPG-SIGNING-KEY-EL
gpgcheck=1
